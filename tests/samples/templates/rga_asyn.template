# This associates an edm screen with the template
# % gui, $(name=), edm, rga.edl, device=$(device)

#% archiver 10 Monitor
#% controldesk Scan Mode smon
#
record(stringin, "$(device):LASTERROR")
{
    field(DESC, "Last Error Response")
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn($(asynport),$(channel))LASTERROR")
    field(SCAN, "I/O Intr")
}

record(ao, "$(device):SCANPERIOD")
    {
    field(DESC, "Bar Chart Scan Period seconds")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(asynport),$(channel))SCANPERIOD")
    field(PINI, "YES")
    field(VAL, "10")
    }

record(ao, "$(device):PJSCANPERIOD")
    {
    field(DESC, "Peak Jump Scan Period seconds")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(asynport),$(channel))PJSCANPERIOD")
    field(PINI, "YES")
    field(VAL, "5")
    }

# Head Connected
#
#
#% archiver 10 Monitor
#
record(stringin, "$(device):HEAD")
{
    field(DESC, "Head")
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn($(asynport),$(channel))HEAD")
    field(SCAN, "I/O Intr")
}
################################################################################
# Head Status
#
record(calc, "$(device):STA")
{
    field(DESC, "Overall status")
    field(CALC, "((A>4)&&(A<=7)&&(B#1))?0:A")
    field(SCAN, "1 second")
    field(INPA, "$(device):HEADSTA PP")
    field(INPB, "$(device):FILSTA PP")
    field(INPC, "$(device):SEV MS")
}
record(calc, "$(device):SEV")
{
    field(DESC, "Persistent Severity")
    field(CALC, "(B=3)?((A>999)?1000:(A+1)):0")
    field(SCAN, "1 second")
    field(INPA, "$(device):SEV.VAL")
    field(INPB, "$(device):HEADSTA.SEVR")
    field(HIGH, "30")
    field(HIHI, "120")
    field(HSV,  "MAJOR")
    field(HHSV, "INVALID")
}


