# simulation_rga.db
# Template database for an RGA
# Pete Owens - 24-6-04
#
# This file is the same as rga.db with DTYP fields commented out
# Records with subsystem :SIM: are to simulate the hardware
#
# substitutions:
# device  - device name
# port    - serial port on IOC (steam device code ie /ty/0/1 => ty_0_1)
# channel - RGA channel number (port number of PC comm port 1-4)
################################################################################
# Simulate random flutuating pressure
# 
record(sub,"$(device):SIM:P") 
{
    field(INAM, "pressRanInit")
    field(SNAM, "pressRanProcess")
    field(INPA, "-11")
    field(INPB, "-10")
}
record(sub,"$(device):SIM:PEAK") 
{
    field(INAM, "pressRanInit")
    field(SNAM, "pressRanProcess")
    field(INPA, "-6")
    field(INPB, "-2")
}
################################################################################
# Simulate staus
# put on a slowish scan to simulate sluggish response
#
record(calc, "$(device):SIM:DATA")
{
    field(DESC, "Simulated Data")
    field(CALC, "A=5?1:(A=6?2:(A=7?3:(A=8?4:0)))")
    field(SCAN, "2 second")
    field(INPA, "$(device):STA")
}
################################################################################
# Head Connected
#
#
#% archiver 10 Monitor
#
record(stringin, "$(device):HEAD")
{
    field(DESC, "Head")
#    field(DTYP, "stream")
#    field(INP,  "@rga.protocol head($(channel)) $(port) 0")
field(VAL,"Simulation")
    field(SCAN, "10 second")
}
################################################################################
# Head Status
#
record(calc, "$(device):STA")
{
    field(DESC, "Overall status")
    field(CALC, "((A>4)&&(A<7)&&(B#1))?0:A")
    field(SCAN, "1 second")
    field(INPA, "$(device):HEADSTA PP")
    field(INPB, "$(device):FILSTA PP")
    field(INPC, "$(device):SEV MS")
}
record(calc, "$(device):SEV")
{
    field(DESC, "Persistent Severity")
    field(CALC, "(B=3)?((A>999)?1000:(A+1)):0")
    field(SCAN, "1 second")
    field(INPA, "$(device):SEV.VAL")
    field(INPB, "$(device):HEADSTA.SEVR")
    field(HIGH, "30")
    field(HIHI, "120")
    field(HSV,  "MAJOR")
    field(HHSV, "INVALID")
}
